@using TechJobs.Models;
@using TechJobs.ViewModels;
@model DisplayJobViewModel

<table class="job-listing">
    <tr>
        <th>Name</th>
        <td>@Model.job.Name</td>
    </tr>
    <tr>
        <th>Employer</th>
        <td>@Model.job.Employer</td>
    </tr>
    <tr>
        <th>Location</th>
        <td>@Model.job.Location</td>
    </tr>
    <tr>
        <th>Skill</th>
        <td>@Model.job.CoreCompetency</td>
    </tr>
    <tr>
        <th>Position Type</th>
        <td>@Model.job.PositionType</td>
    </tr>
</table>